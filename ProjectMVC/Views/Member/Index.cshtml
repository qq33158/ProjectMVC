@model List<Member>
<h1>成員清單</h1>
<div class="row">
    <div class="col-md-12">
    <table class="table table-striped table-bordered">
        <thead>
             <tr>
                <th>序號</th>
                <th>姓名</th>
                <th>Mail</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model.OrderBy(u  => u.Id))
            {
         
                <tr>
                    <td>@obj.Id</td>
                    <td>@obj.Name</td>
                    <td>@obj.Mail</td>
                </tr>
            }
        </tbody>
    </table>
    </div>
</div>